<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.salon.beauty.model.mapper.customer.CustomerIdMapper">
    <select id="customerId" parameterType="String" resultType="String">
        SELECT CUSTOMER_ID
        FROM CUSTOMER
        WHERE CUSTOMER_ID=#{customerId}
    </select>
</mapper>