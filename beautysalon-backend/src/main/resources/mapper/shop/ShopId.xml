<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.salon.beauty.model.mapper.shop.ShopIdMapper">
    <select id="shopId" parameterType="String" resultType="String">
        SELECT SHOP_ID AS shopId
        FROM SHOP
        WHERE SHOP_LOGIN_ID=#{shopLoginId}
    </select>
</mapper>